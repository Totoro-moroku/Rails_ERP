<p id="notice"><%= notice %></p>

<h1>Sublist Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sublist_types.each do |sublist_type| %>
      <tr>
        <td><%= sublist_type.name %></td>
        <td><%= link_to 'Show', sublist_type %></td>
        <td><%= link_to 'Edit', edit_sublist_type_path(sublist_type) %></td>
        <td><%= link_to 'Destroy', sublist_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sublist Type', new_sublist_type_path %>
